{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx app/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "migrate:up": "node-pg-migrate --config-file database.json up",
    "migrate:down": "node-pg-migrate --config-file database.json down"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express-jwt": "^6.0.4",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "node-pg-migrate": "^8.0.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
